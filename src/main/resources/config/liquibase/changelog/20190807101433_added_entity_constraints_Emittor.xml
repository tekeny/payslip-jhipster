<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Emittor.
    -->
    <changeSet id="20190807101433-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="last_status_id"
                                 baseTableName="emittor"
                                 constraintName="fk_emittor_last_status_id"
                                 referencedColumnNames="id"
                                 referencedTableName="status"/>

        <addForeignKeyConstraint baseColumnNames="created_by_id"
                                 baseTableName="emittor"
                                 constraintName="fk_emittor_created_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_2"/>

        <addForeignKeyConstraint baseColumnNames="modified_by_id"
                                 baseTableName="emittor"
                                 constraintName="fk_emittor_modified_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_2"/>

    </changeSet>
</databaseChangeLog>
